# platform :ios, '9.0'
source 'https://github.com/CocoaPods/Specs.git'
platform :ios, '9.0'

target 'quickneasy' do
  # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!

  # Pods for quickneasy
  pod 'Firebase/Core'
  pod 'Firebase/Auth'
  pod 'Google/SignIn'

  pod 'Alamofire', '3.2.1'
  pod 'ObjectMapper', '~> 1.0'

end
